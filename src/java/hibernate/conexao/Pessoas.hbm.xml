<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="br.ifmt.dai.escola.Pessoas" table="pessoas">
        <id name="idPessoa" column="idPessoa" type="int">
            <generator class="native"/>
        </id>
        <property name="idade" column="idade"/>
        <property name="primeiroNome" column="primeiroNome"/>
        <property name="ultimoNome" column="ultimoNome"/>
    </class>



</hibernate-mapping>
