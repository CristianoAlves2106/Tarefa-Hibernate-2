<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="br.ifmt.dai.escola.Eventos" table="eventos">
        <id name="idEventos" column="idEvento" type="int">
            <generator class="native"/>
        </id>
        <property name="evento" column="evento" type="string"/>
        <property name="dataEvento" column="data" type="string"/>
       
    </class>


</hibernate-mapping>
